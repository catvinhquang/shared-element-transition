<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.catvinhquang.sharedelementtransition">

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="S.E.T"
        android:theme="@style/Theme.App">
        <activity android:name="com.catvinhquang.sharedelementtransition.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.catvinhquang.sharedelementtransition.DetailActivity"
            android:theme="@style/Theme.App.Translucent" />
    </application>

</manifest>